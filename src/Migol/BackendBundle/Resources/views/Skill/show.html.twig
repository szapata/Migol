<h1>Skill</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Forplayer</th>
            <td>{{ entity.forplayer }}</td>
        </tr>
        <tr>
            <th>Formanager</th>
            <td>{{ entity.formanager }}</td>
        </tr>
        <tr>
            <th>Fortrainer</th>
            <td>{{ entity.fortrainer }}</td>
        </tr>
        <tr>
            <th>Forfan</th>
            <td>{{ entity.forfan }}</td>
        </tr>
        <tr>
            <th>Formedic</th>
            <td>{{ entity.formedic }}</td>
        </tr>
        <tr>
            <th>Foragent</th>
            <td>{{ entity.foragent }}</td>
        </tr>
        <tr>
            <th>Forexecutive</th>
            <td>{{ entity.forexecutive }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_migol_skill') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_migol_skill_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_migol_skill_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
